{
  "name": "@disappearing-messages/js-isort",
  "version": "0.0.1",
  "description": "Group and sort JS imports.",
  "bin": {
    "js-isort": ".build/src/cli.js"
  },
  "scripts": {
    "build": "rm -rf .build && tsc && chmod +x .build/src/cli.js",
    "check": "npx npm-check --skip-unused",
    "delta-lint:code": "eslint $(git diff --name-only --diff-filter=ACMRTUXB origin/main | grep -E '@/js-isort/.*(.js$|.ts$|.tsx$)' | sed -e 's:@/[^/]*/::g')",
    "lint:code": "eslint .",
    "test": "NODE_ENV=test jest",
    "ts-node": "ts-node -r tsconfig-paths/register",
    "types": "tsc --noEmit"
  },
  "dependencies": {
    "@babel/generator": "^7.22.3",
    "@babel/parser": "^7.22.4",
    "commander": "^11.1.0",
    "tsconfig-resolver": "^3.0.1"
  },
  "devDependencies": {
    "@babel/types": "^7.22.4",
    "@types/jest": "^29.2.2",
    "jest": "^29.3.1",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1"
  }
}
