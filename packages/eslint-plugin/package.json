{
  "name": "@disappearing-messages/eslint-plugin",
  "version": "0.0.1",
  "main": ".build/index.js",
  "scripts": {
    "build": "rm -rf .build && tsc",
    "check": "npx npm-check --skip-unused",
    "delta-lint:code": "eslint $(git diff --name-only --diff-filter=ACMRTUXB origin/main | grep -E '@/eslint-plugin/.*(.js$|.ts$|.tsx$)' | sed -e 's:@/[^/]*/::g')",
    "lint:code": "eslint .",
    "types": "tsc --noEmit"
  },
  "peerDependencies": {
    "eslint": "^8.34.0"
  },
  "devDependencies": {
    "@typescript-eslint/utils": "^6.20.0"
  }
}
